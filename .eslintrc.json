{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-restricted-globals": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 0,
    "no-sequences": 0,
    "no-undef": 0,
    "no-promise-executor-return": 0,
    "consistent-return": 0,
    "prefer-destructuring": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "prefer-template": 0,
    "radix": 0
  }
}
